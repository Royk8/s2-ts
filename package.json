{
  "name": "s2-typescript",
  "version": "0.3.1",
  "description": "S2 standard library for typescript for S2 comunication in a smart grid",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --format cjs,esm --dts && tsc-alias",
    "lint": "tsc",
    "build-clean": "npm run clean && npm run build",
    "release": "npm run build-clean && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Royk8/s2-ts.git"
  },
  "keywords": [
    "s2",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.28",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.8",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@types/uuid": "^9.0.8",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  }
}
